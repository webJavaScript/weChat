page , .map-wrap{
    width: 100%;
    height: 100%;
    overflow: hidden;
    background: #fff;
}
.map{
    width: 100%;
    height: 80%;
}
.map-info{
    padding: 40rpx 30rpx;
    height: 50rpx;
}
.icon-address{
    width: 28rpx;
    height: 30rpx;
    margin-right: 20rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}
.icon-addressinfo{
    margin-top: 3rpx;
    width: 28rpx;
    height: 30rpx;
    margin-right: 20rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}
.map-info-name{
    max-width: 600rpx;
    line-height: 36rpx;
    font-size: 34rpx;
    color: #333333;
    height: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.map-info-address{
    color: #666666;
    font-size: 28rpx;
    line-height: 36rpx;
}