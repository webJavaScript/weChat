.qd-wrap{
    width: 100%;
}
.qd{
    overflow-y: auto;
    padding-top: 40rpx;
    padding-bottom: 200rpx; 
}
.qd-bottom{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 160rpx;
    border-top: 1rpx solid #e1e1e1;
    background: #fff;
}
.qd-bottom-left{
    position: absolute;
    top: 0;
    left: 0;
     z-index: 1; 
    width: 86rpx;
    height: 100%;
    box-sizing: border-box;
    background: #fff;
    border-right: 1rpx solid #e1e1e1;
    color: #ffa800;
    font-size: 30rpx;
    padding-top: 40rpx;
    line-height: 40rpx;
    text-align: center;
}

.qd-bottom-left::before{
    content: '';
    display: block;
    position: absolute;
    width: 16rpx;
    height: 16rpx;
    background:#fff;
    transform: rotateZ(45deg);
    top: 50%;
    margin-top: -11rpx;
    right: -7rpx;
    border-right: 1rpx solid #e1e1e1;
    border-top: 1rpx solid #e1e1e1;

}
.qd-bottom-save{
    position: fixed;
    bottom: 200rpx;
    right: 20rpx;
    z-index: 1; 
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    display: flex;
    align-items: center;
    box-sizing: border-box;
    flex-direction: column;
    justify-content: center;
    background: #e6e6e6 url(data:image/png;base64,) center no-repeat;
    background-size: 100%;
    /*background: rgba(39, 194, 73, .8);*/
    color: #fff;
    font-size: 24rpx;
    text-align: center;
}
.qd-bottom-saved {
    background-color: #fff;
    background-image: url(data:image/png;base64,);
}
.qd-bottom-right{
    white-space: nowrap;
}
.qd-bottom-ls{
    display: inline-block;
    width: 90rpx;
    padding: 17rpx 25rpx 0;
}
.qd-bottom-ls:first-child{
    padding-left: 111rpx; 
}
.qd-bottom-ls-imgwrap{
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    overflow: hidden;
}
.qd-bottom-ls-name{
    text-align: center;
    line-height: 48rpx;
    color: #333333;
    font-size: 24rpx;
}
.qd-ls-time{
    font-size: 20rpx;
    color: #666666;
    line-height: 22rpx;
    margin-bottom: 30rpx;
    text-align: center;
}
.qd-lsu-wrap{
    padding: 0 30rpx;
}
.qd-lsu-main::before{
    content: '';
    display: block;
    position: absolute;
    right: -6rpx;
    top: 25rpx;
    width: 14rpx;
    height: 14rpx;
    background: #6595ff;
    transform: rotateZ(45deg);
}
.qd-lsu-main{
    position: relative;
    max-width: 550rpx;
    border-radius: 8rpx;
    background: #6595ff;
    padding: 20rpx 30rpx; 
    line-height: 48rpx;
    font-size: 30rpx;
    color: #fff;
}
.qd-lsum-title-hl{
    color: #ffa800;
}
.qd-lsd-main{
    display: flex;
}
.qd-lsdm-left{
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-right: 37rpx;
}
.qd-lsdm-right{
    flex: 1;
}
.qd-ls-user{
    margin-bottom: 40rpx;
}
.qd-lsdmr-title{
    line-height: 36rpx;
    font-size: 24rpx;
    max-width: 450rpx;
}
.qd-lsdmr-name{
    display: inline;
    color: #6595ff;
    margin-right: 10rpx;
}
.qd-lsdmr-job{
    display: inline;
    color: #666666;
    margin-right: 10rpx;
}
.qd-lsdmr-hos{
    display: inline;
    color:#ffa800;
}
.qd-lsdmr-tele{
    line-height: 36rpx;
    color: #666666;
    font-size: 24rpx;
}
.qd-lsdmr-tele-hl{
    margin-left: 10rpx;
    color: #ff3b30;
}
.qd-lsdmr-main{
    position: relative;
    padding: 20rpx 30rpx;
    background: #fff;
    max-width: 450rpx;
    margin-top: 10rpx;
    border: 1rpx solid #e1e1e1; 
    border-radius: 8rpx;
    color: #333333;
    line-height: 48rpx;
    font-size: 30rpx;
    word-break: break-all;
}
.qd-lsdmr-main::before{
    content: '';
    display: block;
    position: absolute;
    left: -7rpx;
    top: 25rpx;
    width: 14rpx;
    height: 14rpx;
    background: #fff;
    border-bottom: 1rpx solid #e1e1e1;
    border-left: 1rpx solid #e1e1e1;
    transform: rotateZ(45deg);
}
.qd-lsdmr-main-add{
    color: #6595ff;
}
.qd-lsdmr-main-had{
    content: '';
    display: block;
    position: absolute;
    right: 0;
    top: 0;
    line-height: 26rpx;
    font-size: 22rpx;
    color: #fff;
    background: #ffa800;
    padding-left: 15rpx;
    padding-right: 5rpx;
    border-radius: 0 8rpx 0 16rpx;
}
.qd-ls-doctor{
    margin-bottom: 30rpx;
}
.qd-lsum-content{
    word-break: break-all;
}

