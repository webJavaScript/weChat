.fd-wrap{
    border-bottom: 1rpx solid #e1e1e1;
    background: #fff;
    /*padding: 40rpx 0;*/
    display: flex;
}
.fd-ls{
    display: flex;
    flex: 1;
    position: relative;
    padding: 56rpx 0 56rpx 56rpx;
}

.fd-ls:first-child .fd-ls-right {
    border-right: 1rpx solid #ededed;
}

.icon-fd-hospital{
    width: 77rpx;
    height: 70rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    margin-right: 25rpx;
}
.icon-fd-department{
    width: 60rpx;
    height: 70rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
    margin-right: 25rpx;
}
.fd-ls-right{
    flex: 1;
}
.fd-lr-title{
    font-size: 32rpx;
    color: #333333;
    line-height: 34rpx;
    margin-bottom: 8rpx;
}
.fd-lr-info{
    color: #999999;
    font-size: 26rpx;
    line-height: 30rpx;
}
.hot-department{
    background: #fff;
    padding: 0 30rpx 40rpx;
    border-bottom: 1rpx solid #dcdcdc; 
}
.hot-department-title{
    color: #333333;
    font-size: 30rpx;
    line-height: 88rpx;
}
.hot-department-ls{
    line-height: 64rpx;
    padding: 0 34rpx;
    background: #f5f5f5;
    border-radius: 32rpx;
    font-size: 28rpx;
    color: #666666;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}
.hot-department-main{
    margin-bottom: -20rpx;
}













