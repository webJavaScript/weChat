@import "./templates/loading/loading.wxss";
@import "./templates/plain/plain.wxss";

/**app.wxss**/
.fl {
    float: left;
}

.fr {
    float: right;
}

.clearfix::after {
    content: '';
    display: block;
    clear: both;
}

page {
    background-color: #f8f8f8;
    color: #333;
}
.all-img{
    width: 100%;
}


.pt-100 {
    padding-top: 100rpx;
}
.mt-20{
    margin-top:20rpx;
}
.mb-20 {
    margin-bottom: 20rpx;
}

.btn-default {
    width: 100%;
    line-height: 88rpx;
    background-color: #000;
}
.m-t {
    margin-top: 20rpx;
}
.m-t-lg {
    margin-top: 30rpx;
}
.m-l-lg {
    margin-left: 30rpx;
}
.m-l {
    margin-left: 20rpx;
}
.m-l-xs {
    margin-left: 10rpx;
}
.m-r-lg {
    margin-right: 30rpx;
}
.m-t-xs {
    margin-top: 10rpx;
}
.m-b {
    margin-bottom: 20rpx;
}
.fz {
    font-size: 30rpx;
}
.fz-xs {
    font-size: 28rpx;
}
.fz-xss {
    font-size: 24rpx;
}
.fz-lg {
    font-size: 32rpx;
}
.fz-lgx {
    font-size: 36rpx;
}
.col-666 {
    color: #666;
}
.col-fff {
    color: #fff;
}
.flx-1 {
    flex: 1;
}

.col-title {
    border-bottom: 1rpx solid #e1e1e1;
    padding: 0 30rpx;
    background-color: #fff;
    line-height: 80rpx;
}
.col-title-h {
    font-size: 30rpx;
}
.col-title-desc {
    display: inline;
    font-size: 24rpx;
    color: #999;
}
.bg-white, .uc-iask-items {
    background: #fff;
}
.line-clamp-1 {
    display: -webkit-box;
    -webkit-line-clamp: 1;
    line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.fdh-wrap{
    position: relative;
}
.fdh{
    width: 100%;
    padding-top: 89rpx;
}
.fdh-title{
    padding: 0 30rpx;
    line-height: 89rpx;
    font-size: 28rpx;
    color: #666666;
}

.wbwr {
    word-break: break-all;
    word-wrap: break-word;
}

.btn-more {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx;
    background-color: #fff;
}
.btn-more-txt {
    font-size: 30rpx;
    color: #999;
}
.btn-more-arrow image {
    width: 11rpx;
    height: 20rpx;
}
.null {
    padding: 30rpx 0;
    text-align: center;
    font-size: 30rpx;
    color: #c2c2c2;
}

.image-pre {
    background: #f5f5f5 url(data:image/png;base64,) center center no-repeat;
    background-size: auto 70%;
}
