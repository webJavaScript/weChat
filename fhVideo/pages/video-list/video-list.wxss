@import "../../utils/template/video-list.wxss";

.loading {
    font-size: 40rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    margin-top: 4em;
    color: #999999;
}
.loading .loading-img {
    width: 12em;
    height: 12em;
    margin-bottom: 2em;
}
.loading .loading-img image {
    max-width: 100%;
    max-height: 100%;
}
